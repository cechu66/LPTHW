http://zetcode.com/web/pythonrequests/

import requests, urllib3

urllib3.disable_warnings(urllib3.exceptions.InsecureRequestWarning)

data=[]
websites=['http://rainmaker-labs.com']
for w in websites:
    r = requests.get(w, verify=False)
    r2 = requests.head(w, verify=False)
    data.append( (r.url, len(r.content), r.elapsed.total_seconds(), str([(l.status_code, l.url) for l in r.history]), str(r.headers['server']) ))
    print(data)
    if (str(r.headers['server']) == "nginx"):
        print("found")
